{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mejorve-frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "assets/favicon.gif",
        {
          "glob": "**/*",
          "input": "../node_modules/datatables/media/images",
          "output": "./assets/images/datatables/"
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/datatables/media/css/jquery.dataTables.css",
        "../node_modules/icheck/skins/all.css",
        "../node_modules/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css",
        "./assets/toastmessage/src/main/resources/css/jquery.toastmessage.css",
        "styles.scss"
      ],
      "stylePreprocessorOptions": {
        "includePaths": [
          "style-paths"
        ]
      },
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "global_scripts.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/datatables/media/js/jquery.dataTables.js",
        "../node_modules/icheck/icheck.js",
        "../node_modules/moment/min/moment.min.js",
        "../node_modules/moment/min/locales.min.js",
        "../node_modules/gasparesganga-jquery-loading-overlay/src/loadingoverlay.js",
        "../node_modules/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js",
        "../node_modules/deepmerge/index.js",
        "./assets/toastmessage/src/main/javascript/jquery.toastmessage.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "class": {
      "spec": true
    },
    "component": {
      "spec": true
    },
    "directive": {
      "spec": true
    },
    "module": {
      "spec": true
    },
    "pipe": {
      "spec": true
    },
    "service": {
      "spec": true
    }
  }
}
